@model IEnumerable<IsmIoTPortal.Models.Command>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IsmDevice.DeviceId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cmd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Timestamp)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CommandStatus)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IsmDevice.DeviceId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cmd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Timestamp)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CommandStatus)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CommandId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CommandId }) |
